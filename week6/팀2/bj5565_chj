package week06.íŒ€2;

import java.util.Scanner;

public class bj5565_chj {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int price = in.nextInt();
		
		int arr[] = new int[9];
		for(int i = 0; i<9; i++) {
			arr[i] = in.nextInt();
			price = price -arr[i];
		}
		System.out.println(price);

	}

}
